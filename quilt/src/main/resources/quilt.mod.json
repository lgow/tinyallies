{
  "schema_version": 1,
  "mixin": [
    "tinyallies.mixins.json",
    "tinyallies-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "tinyallies",
    "version": "${version}",
    "metadata": {
      "name": "Tiny Allies",
      "contributors": {
        "Lgowz": "Owner"
      },
      "description": "Tiny Allies adds tamable baby versions of the classic vanilla monsters!",
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "license": {
        "name": "All Rights Reserved",
        "id": "ARR",
        "url": ""
      },
      "icon": "assets/tinyallies/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.tinyallies.quilt.TinyAlliesQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.4"
      },
      {
        "id": "architectury",
        "version": ">=8.1.79"
      }
    ]
  }
}